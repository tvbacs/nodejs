{
  "name": "your-app-name",
  "version": "1.0.0",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "mysql": "^2.18.1"
  },
  "scripts": {
    "start": "node backend/app.js"
  },
  "vercel": {
    "version": 2,
    "builds": [
      { "src": "index.html", "use": "@vercel/static-build" },
      { "src": "backend/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/", "dest": "index.html" },
      { "src": "/api/.*", "dest": "backend/app.js" }
    ]
  }
}