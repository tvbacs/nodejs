{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/*",
      "dest": "/public/:splat"
    },
    {
      "src": "/assets/*",
      "dest": "/assets/:splat"
    },
    {
      "src": "/*",
      "dest": "/app.js"
    }
  ]
}