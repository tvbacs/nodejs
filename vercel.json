{
    "version": 2,
    "name": "my-app",
    "builds": [
      {
        "src": "backend/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/app.js"
      }
    ],

    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/"
        }
      ]
  }
  