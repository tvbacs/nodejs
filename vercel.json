{ "version": 2, 
"name": "my-app", 
"routes": [
  {
    "src": "/",
    "dest": "/public/index.html"
  },
  {
    "src": "/(.*)",
    "dest": "/backend/app.js"
  }
],
"builds": [ { 
  "src": "backend/app.js", 
  "use": "@vercel/node" } ], 
  "rewrites": [ { "source": "/(.*)", 
  "destination": "/backend/app.js" } ] } 