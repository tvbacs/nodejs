{
  "builds": [
    { "src": "backend/app.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/backend/app.js" },
    { "source": "index.html", "destination": "index.html" }
  ],
  "version": 2,
  "name": "my-app",
  "routes": [
    { "src": "/(my)", "dest": "/backend/app.js" }
  ]
}
