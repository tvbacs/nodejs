{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {}
    },
    {
      "src": "backend/app.js",
      "use": "@vercel/node",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/api/*",
      "dest": "backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}