{
  "version": 2,
  "builds": [
    {
      "src": "/api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/static_file.js",
      "use": "@vercel/node"
    }

  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/static_file.js"
    }
  ]
}
